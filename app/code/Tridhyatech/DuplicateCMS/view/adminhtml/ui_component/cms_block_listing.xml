<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="duplicateblock">
                <settings>
                    <confirm>
                        <message translate="true">Are you sure you want to duplicate selected items?</message>
                        <title translate="true">Duplicate items</title>
                    </confirm>
                    <url path="duplicatecms/block/massDuplicate"/>
                    <type>duplicateblock</type>
                    <label translate="true">Duplicate</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
